package model;

import java.io.File;

import junit.framework.TestCase;

public class TestDataSet extends TestCase {

	DataSet ds;
	
	
	protected void setUp() throws Exception {
		ds=new DataSet();
	}
	
	public void testDataSetInit(){
		assertNotNull(ds);
		assertNotNull(ds.dataset);
	}

	public void testLoadData(){
		assertTrue(ds.dataset.size()==0);
		String filePath = File.separator+"resources"+File.separator+"dataToBeLoad.txt";
		ds.loadDataSetFromFile(filePath);
		assertTrue(ds.dataset.size()==6);
	}
}
