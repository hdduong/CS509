package model;

import model.Graph.GRAPH_TYPE;

public class CurrentSystemState {
	 
	DataSet dataset;
	Graph graph;
	GRAPH_TYPE currentGraphType;
	
	//initilize the system
	CurrentSystemState(){
		dataset=new DataSet();
		graph = new CartesianPlot();
		currentGraphType = GRAPH_TYPE.CARTESIAN;
	}
}
